@model IEnumerable<IntegrationProject.Models.User>
@*@model IntegrationProject.Models.User*@

@{
    ViewBag.Title = "UserSignedInIndex";
    Layout = "~/Views/Shared/_LayoutVolunteer.cshtml";
}

<h2>Volunteer Profile</h2>

<div>
    @*<h4>User</h4>*@
    <hr />
@foreach (var item in Model)
{
    <dl class="dl-horizontal">
        <dt>
            Name
            @*@Html.DisplayNameFor(model => model.FirstName)*@
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.FirstName) @Html.DisplayFor(modelItem => item.LastName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.Email)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Phone)
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.Phone)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Description)
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.Description)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.VolunteerUpVotes)
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.VolunteerUpVotes)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.VolunteerDownVotes)
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.VolunteerDownVotes)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.EventUpVotes)
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.EventUpVotes)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.EventDownVotes)
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.EventDownVotes)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.NoShowCount)
        </dt>

        <dd>
            @Html.DisplayFor(modelItem => item.NoShowCount)
        </dd>       
    </dl>
    <p>
        @Html.ActionLink("Edit Profile", "Edit", new { id = item.ID })
    </p>
}
</div>



